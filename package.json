{
  "name": "tab-group-helper",
  "prettyName": "Tab Group Helper",
  "description": "Allow quick grouping of tabs by their attributes like title or URL",
  "version": "1.8.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedidomizio/tab-group-helper"
  },
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,css,html}'",
    "test": "jest --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "test-all": "node ./scripts/run-all-tests-for-coverage.mjs",
    "prep-publish": "node ./scripts/prepare-publish.mjs",
    "publish": "node ./scripts/publish-updated-package-to-chrome-webstore.mjs",
    "build-ext": "sh ./scripts/build.sh"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/react-dom": "^17.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.1"
  },
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/chrome": "0.0.164",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/sinon-chrome": "^2.2.11",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.31.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "chrome-webstore-upload": "^0.6.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-loader": "^2.1.0",
    "html-webpack-plugin": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^27.2.5",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "puppeteer": "^10.4.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "sass-loader": "^11.0.1",
    "sinon-chrome": "^3.0.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.4.3",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^react($|/.+)": "<rootDir>/node_modules/react$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/reportWebVitals.js",
      "!src/index.js",
      "!src/__tests-helpers__/"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 30,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
